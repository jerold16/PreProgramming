import java.util.Arrays;
import java.util.Scanner;

public class Add_or_Even {
	public static void main(String[] args) {
		int arr[]= {1,9,3,5,4,6,7,8,2};
		Arrays.sort(arr);
		int temp[] = null;
		for(int i=0;i<=arr.length;i++)
		{
			if(arr[i]%2==0)
			{
				temp[i]++;
			}
			temp[i]++;	
			System.out.println(temp[i]);
		}
	}
}
