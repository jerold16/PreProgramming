import java.util.Scanner;

public class MagicNumber {
	static int Magic(int num) {
		int sum=0;
		
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		int mnum=Magic(num);
		while(mnum>9) {
			mnum=Magic(num);
		}
		if(mnum==1)
			System.out.println("Magic number");
		else
			System.out.println("Not");
	}

}
