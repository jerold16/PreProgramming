package Leetcode;

public class FirstNonRepeat {
	static void nonrepeat(String str) {
		
		for(int i=0;i<str.length();i++) {
			char ch=str.charAt(i);
			if(str.indexOf(ch)==str.lastIndexOf(ch))
				System.out.println(i);
		}
		
	}
	public static void main(String[] args) {
		String str="abcdac";
		nonrepeat(str);
	}

}
